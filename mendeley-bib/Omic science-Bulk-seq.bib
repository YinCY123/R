doi = {10.1038/ng.3520},
file = {:C$\backslash$:/Users/YinCY/Documents/Mendeley Desktop/Bueno et al/Nature Genetics/Bueno et al. - 2016 - Comprehensive genomic analysis of malignant pleural mesothelioma identifies recurrent mutations, gene fusions and.pdf:pdf},
