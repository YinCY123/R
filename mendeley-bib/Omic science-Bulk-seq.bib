journal = {Nature biotechnology},
number = {5},
pages = {511--5},
pmid = {20436464},
publisher = {Nature Publishing Group},
title = {{Transcript assembly and quantification by RNA-Seq reveals unannotated transcripts and isoform switching during cell differentiation.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20436464{\%}0Ahttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3146043},
