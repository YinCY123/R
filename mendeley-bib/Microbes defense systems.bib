eprint = {arXiv:1408.1149},
file = {:C$\backslash$:/Users/YinCY/Documents/Mendeley Desktop/Kaya et al/Proceedings of the National Academy of Sciences/Kaya et al. - 2016 - A bacterial Argonaute with noncanonical guide RNA specificity.pdf:pdf},
isbn = {0027-8424},
