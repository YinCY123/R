isbn = {1091-6490 (Electronic)$\backslash$r0027-8424 (Linking)},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
number = {2},
pages = {652--657},
pmid = {24374628},
title = {{Structure-based cleavage mechanism of Thermus thermophilus Argonaute DNA guide strand-mediated DNA target cleavage}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1321032111},
volume = {111},
