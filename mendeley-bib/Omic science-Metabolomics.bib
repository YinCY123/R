eprint = {15334406},
file = {:C$\backslash$:/Users/YinCY/Documents/Mendeley Desktop/Uppal, Walker, Jones/Analytical Chemistry/Uppal, Walker, Jones - 2017 - xMSannotator An R package for network-based annotation of high-resolution metabolomics data.pdf:pdf;:C$\backslash$:/Users/YinCY/Documents/Mendeley Desktop/Uppal, Walker, Jones/Analytical Chemistry/Uppal, Walker, Jones - 2017 - xMSannotator An R package for network-based annotation of high-resolution metabolomics data(2).pdf:pdf},
isbn = {0003-2700},
