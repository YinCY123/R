@article{Lu2017,
abstract = {Metabolites are the small biological molecules involved in energy conversion and biosynthesis. Studying metabolism is inherently challenging due to the reactivity, structural diversity, and broad concentration range of metabolites. Herein, we review the common pitfalls encountered in preparing and analyzing metabolomics samples and provide concrete guidelines for obtaining accurate measurements. We focus on the analysis of water-soluble primary metabolites, which carry the majority of metabolic flux in living organisms. We show how seemingly straightforward sample preparation methods can introduce systematic errors (e.g., due to interconversion among metabolites) and how proper selection of quenching solvent (e.g. acidic acetonitrile: methanol: water) can mitigate such problems. We discuss each common analytical platform (LC-MS, GC-MS, NMR, and enzyme assays) with a focus on its specific pitfalls and how to avoid them. In so doing, we provide the knowledge base for carrying out biologically informative metabolite measurements.},
author = {Lu, Wenyun and Su, Xiaoyang and Klein, Matthias S. and Lewis, Ian A. and Fiehn, Oliver and Rabinowitz, Joshua D.},
