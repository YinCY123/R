pages = {277--304},
pmid = {28654323},
title = {{Metabolite Measurement: Pitfalls to Avoid and Practices to Follow}},
url = {http://www.annualreviews.org.sire.ub.edu/doi/abs/10.1146/annurev-biochem-061516-044952{\%}5Cnhttp://www.annualreviews.org/doi/10.1146/annurev-biochem-061516-044952 http://www.annualreviews.org/doi/10.1146/annurev-biochem-061516-044952},
